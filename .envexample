#DEBUG is the most important setting used natively by Django, set it to False if this is production environment
DEBUG='True'

# ENV is 'local' or 'production'. It indicates if we use gunicorn and nginx
ENV=local

#DATABASE
DATABASE_ENGINE=django.db.backends.postgresql
DATABASE_NAME=
DATABASE_HOST=
DATABASE_PORT=5432
